                    app:layout_constraintTop_toBottomOf="@+id/et_infraestructura" />

                <EditText
                    android:id="@+id/et_infraestructura"
                    android:layout_width="379dp"
                    android:layout_height="60dp"
                    android:ems="10"
                    android:hint="Infraestructura"
                    android:inputType="textPersonName"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/et_actividadesDelArea" />

                <EditText
                    android:id="@+id/et_actividadesDelArea"
                    android:layout_width="379dp"
                    android:layout_height="60dp"
                    android:ems="10"
                    android:hint="Actividades del área"
                    android:inputType="textPersonName"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/et_fauna" />

                <EditText
                    android:id="@+id/et_fauna"
                    android:layout_width="379dp"
                    android:layout_height="60dp"
                    android:ems="10"
                    android:hint="Fauna Emblemática"
                    android:inputType="textPersonName"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/et_flora" />

                <EditText
                    android:id="@+id/et_flora"
                    android:layout_width="379dp"
                    android:layout_height="60dp"
                    android:ems="10"
                    android:hint="Flora Emblemática"
                    android:inputType="textPersonName"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/et_clima" />

                <EditText
                    android:id="@+id/et_clima"
                    android:layout_width="379dp"
                    android:layout_height="60dp"
                    android:ems="10"
                    android:hint="Clima"
                    android:inputType="textPersonName"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/et_geologia" />

                <EditText
                    android:id="@+id/et_geologia"
                    android:layout_width="379dp"
                    android:layout_height="60dp"
                    android:ems="10"
                    android:hint="Geología"
                    android:inputType="textPersonName"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/et_superficie" />

                <EditText
                    android:id="@+id/et_superficie"
                    android:layout_width="379dp"
                    android:layout_height="60dp"
                    android:ems="10"
                    android:hint="Superficie"
                    android:inputType="textPersonName"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/et_geolocalizacion" />

                <EditText
                    android:id="@+id/et_geolocalizacion"
                    android:layout_width="379dp"
                    android:layout_height="60dp"
                    android:ems="10"
                    android:hint="Geolocalización"
                    android:inputType="textPersonName"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/et_caracteristicasGenerales" />

                <EditText
                    android:id="@+id/et_caracteristicasGenerales"
                    android:layout_width="379dp"
                    android:layout_height="60dp"
                    android:ems="10"
                    android:hint="Características Generales"
                    android:inputType="textPersonName"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/et_fechaCreacion" />

                <EditText
                    android:id="@+id/et_fechaCreacion"
                    android:layout_width="379dp"
                    android:layout_height="60dp"
                    android:ems="10"
                    android:hint="Fecha de creación"
                    android:inputType="textPersonName"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/et_importancia" />

                <EditText
                    android:id="@+id/et_importancia"
                    android:layout_width="379dp"
                    android:layout_height="60dp"
                    android:ems="10"
                    android:hint="importancia"
                    android:inputType="textPersonName"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/et_acceso" />

                <EditText
                    android:id="@+id/et_instrumentoLegal"
                    android:layout_width="379dp"
                    android:layout_height="60dp"
                    android:ems="10"
                    android:hint="Instrumento legal"
                    android:inputType="textPersonName"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.495"
                    app:layout_constraintStart_toStartOf="parent"
                    tools:layout_editor_absoluteY="160dp" />

                <EditText
                    android:id="@+id/et_acceso"
                    android:layout_width="379dp"
                    android:layout_height="60dp"
                    android:ems="10"
                    android:hint="Acceso"
                    android:inputType="textPersonName"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/et_instrumentoLegal" />
            </androidx.constraintlayout.widget.ConstraintLayout>
        </LinearLayout>
    </ScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>